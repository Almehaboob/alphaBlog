<!-- app/views/users/new.html.erb -->
<h1 style="margin-left: 50%;"> <%= @user.username %> profile</h1>

<div class="text-center mt-3"> 
    <%= link_to "edit your profile",edit_user_path(@user), class: 'btn btn-outline-info'  %>
</div>
<div class="text-center mt-3"> 
    <%= link_to "delete profile", user_path(current_user), method: :delete, class: 'btn btn-outline-danger' %>

</div>

<h3>Articles </h3>

<%= render 'articles/article' %>